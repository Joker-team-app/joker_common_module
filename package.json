{
  "name": "joker-common-module",
  "private": true,
  "version": "1.0.0",
  "main": "joker-common-module.mjs",
  "module": "joker-common-module.mjs",
  "types": "main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/joker-common-module.mjs",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:types": "dts-bundle-generator --external-inlines -o dist/main.d.ts src/main.ts",
    "build": "yarn run build:types && vite build --mode staging",
    "build:prod": "yarn run build:types && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "1.9.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "2.6.1",
    "@types/crypto-js": "4.2.2",
    "crypto-js": "4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "9.2.0",
    "react-router-dom": "7.6.1",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.0",
    "@types/dts-bundle": "^0",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.4.1",
    "dts-bundle-generator": "^8.0.0",
    "eslint": "9.25.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
